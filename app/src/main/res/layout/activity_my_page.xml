<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ActivityMyPage">

    <TextView
        android:id="@+id/tv_voucher_detail"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/shape_round_corner_pastel_blue_filled"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6dp"
        android:text="     결제일 | 2021년 11월 30일\n     결제 예정일 | 2022년 11월 30일\n     이용 현황 | 총 30회 중 2회 이용"
        android:textColor="@color/black"
        android:textSize="13dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_name14"
        app:layout_constraintTop_toTopOf="@+id/tv_name19"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/tv_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:lineSpacingExtra="6dp"
        android:text="내 정보"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_menu" />

    <TextView
        android:id="@+id/tv_name14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="25dp"
        android:lineSpacingExtra="6dp"
        android:text="이름"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/tv_name4"
        app:layout_constraintTop_toBottomOf="@+id/tv_name4" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/shape_round_corner_gray_filled"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6dp"
        android:text="     홍길동"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_name14"
        app:layout_constraintTop_toTopOf="@+id/tv_name14"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/tv_voucher"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/shape_round_corner_gray_filled"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6dp"
        android:text="     프리미엄 이용권 (1년권)"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_name14"
        app:layout_constraintTop_toTopOf="@+id/tv_name18" />

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/shape_round_corner_gray_filled"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6dp"
        android:text="     홍길동"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_name14"
        app:layout_constraintTop_toTopOf="@+id/tv_name16" />

    <TextView
        android:id="@+id/tv_phone"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/shape_round_corner_gray_filled"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6dp"
        android:text="     010-1234-5678"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_name14"
        app:layout_constraintTop_toTopOf="@+id/tv_name15" />

    <TextView
        android:id="@+id/tv_name15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:lineSpacingExtra="6dp"
        android:text="연락처"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/tv_name14"
        app:layout_constraintTop_toBottomOf="@+id/tv_name14" />

    <TextView
        android:id="@+id/tv_name16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:lineSpacingExtra="6dp"
        android:text="아이디"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/tv_name15"
        app:layout_constraintTop_toBottomOf="@+id/tv_name15" />

    <TextView
        android:id="@+id/tv_name18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:lineSpacingExtra="6dp"
        android:text="이용권"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/tv_name16"
        app:layout_constraintTop_toBottomOf="@+id/tv_name16" />

    <TextView
        android:id="@+id/tv_name19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:lineSpacingExtra="6dp"
        android:text="상세 내역"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/tv_name16"
        app:layout_constraintTop_toBottomOf="@+id/tv_name18" />

    <TextView
        android:id="@+id/tv_name17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:lineSpacingExtra="6dp"
        android:text="내 차량"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tv_name4"
        app:layout_constraintTop_toBottomOf="@+id/tv_name19" />

    <TextView
        android:id="@+id/tv_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:lineSpacingExtra="6dp"
        android:text="회원 정보"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tv_name2"
        app:layout_constraintTop_toBottomOf="@+id/tv_name2" />

    <Button
        android:id="@+id/btn_menu"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/btn_menu"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/btn_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_back" />

    <Button
        android:id="@+id/btn_back"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btn_back_colored"
        android:backgroundTint="#00BCD4"
        android:backgroundTintMode="src_in"
        android:onClick="onClick"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="170dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/shape_round_corner_dark_gray_filled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_name17">

        <TextView
            android:id="@+id/tv_car_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="10가 1234"
            android:textColor="#CECECE"
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_car_model"
            app:layout_constraintTop_toBottomOf="@+id/tv_car_model" />

        <TextView
            android:id="@+id/tv_car_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:text="Ionic 5"
            android:textColor="@color/white"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="150dp"
            android:layout_height="80dp"
            android:layout_marginStart="70dp"
            app:layout_constraintStart_toEndOf="@+id/tv_car_model"
            app:layout_constraintTop_toTopOf="@+id/tv_car_model"
            app:srcCompat="@drawable/ionic" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>